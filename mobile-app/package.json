{
  "name": "mobile-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "postinstall": "patch-package",
    "dev": "vite",
    "dev:testing": "vite -m testing",
    "dev:presentation": "vite -m presentation",
    "build": "tsc && vite build",
    "build:presentation": "tsc && vite build -m presentation",
    "serve": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "ionic": "ionic",
    "ionic:serve": "vite --host",
    "clean": "rm -rf ./build && rm -rf node_modules && rm -rf ios/app/Pods",
    "mobile:open": "ionic cap open",
    "mobile:add": "ionic cap add",
    "ios": "ionic cap run ios -l --external -p 5173",
    "android": "ionic cap run android -l --external -p 5173",
    "cy:open": "start-server-and-test dev:testing http://localhost:5173 'cypress open'",
    "cy:e2e": "start-server-and-test dev:testing http://localhost:5173 'cypress run --browser chrome'",
    "cy:components": "cypress run --browser chrome --component"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/clipboard": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/device": "^6.0.1",
    "@capacitor/geolocation": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/live-updates": "^0.2.1",
    "@capacitor/push-notifications": "^6.0.2",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic/react": "^8.2.0",
    "@tanstack/react-query": "^5.39.0",
    "capacitor-native-settings": "^6.0.0",
    "firebase": "^10.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-router": "^6.27.0",
    "react-router-dom": "^6.23.1",
    "rollup": "npm:@rollup/wasm-node",
    "socket.io-client": "^4.6.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@ionic/cli": "^7.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-legacy": "^5.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cypress": "^13.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "start-server-and-test": "^2.0.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "terser": "^5.31.0",
    "ts-jest": "^29.1.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "description": "Fleets using Ionic React",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
